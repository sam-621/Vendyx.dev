{
  "compilerOptions": {
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "target": "es2019",
    "strict": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "jsx": "react-jsx"
  },
  "exclude": ["node_modules"]
}
